{
    "version": "0.2.1",
    "configurations": [
        {
            "name": "Debug active test (gcc: link active test)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "gcc: link active test"
        }
    ]
}